{
	"name": "@sudoo/csv",
	"version": "2.1.0",
	"description": "CSV Utils for JS",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SudoDotDog/Sudoo-CSV.git"
	},
	"author": "WMXPY",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/SudoDotDog/Sudoo-CSV/issues"
	},
	"homepage": "https://github.com/SudoDotDog/Sudoo-CSV#readme",
	"devDependencies": {
		"@sudoo/build-utils": "^1.0.0",
		"@sudoo/license": "^1.1.0",
		"@sudoo/triforce": "^2.10.0"
	},
	"nyc": {
		"extension": [
			".ts"
		],
		"include": [
			"script/**/*.ts",
			"src/**/*.ts"
